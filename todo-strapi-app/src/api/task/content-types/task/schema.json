{
  "kind": "collectionType",
  "collectionName": "tasks",
  "info": {
    "singularName": "task",
    "pluralName": "tasks",
    "displayName": "task",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "due_date": {
      "type": "datetime"
    },
    "completed": {
      "type": "boolean",
      "default": false
    },
    "priority": {
      "type": "enumeration",
      "enum": [
        "low",
        "medium",
        "high"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "tasks"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "tasks"
    },
    "worker": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::worker.worker"
    }
  }
}
